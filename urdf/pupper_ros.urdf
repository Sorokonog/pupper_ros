<?xml version="1.0" ?>
<robot name="pupper_ros">

  <link name="torso">
      <visual>
        <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
        <geometry>
          <box size="0.4 0.2 0.05"/>
        </geometry>
        <material name="blue">
          <color rgba="0.3 0.5 0.9 1.0"/>
